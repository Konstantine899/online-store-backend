{
  "name": "online-store-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "lint:ts": "eslint \"{src,config,db}/**/*.ts\"",
    "lint:ts:fix": "eslint \"{src,config,db}/**/*.ts\" --fix",
    "prettier": "prettier --write \"src/**/*.ts\" \"db/**/*.{js,ts}\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:unit:watch": "jest --selectProjects unit --watch",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:cov": "jest --coverage",
    "test:cov:unit": "jest --selectProjects unit --coverage",
    "test:cov:integration": "jest --selectProjects integration --coverage",
    "test:cov:open": "npm run test:cov && start coverage/index.html",
    "test:html": "npm run test && start test-reports/test-report.html",
    "test:html:open": "npm run test && start test-reports/test-report.html",
    "test:ci": "cross-env CI=true NODE_ENV=test jest",
    "test:debug": "jest --detectOpenHandles --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:build": "tsc -p db/tsconfig.json",
    "db:lint": "eslint \"db/**/*.ts\"",
    "db:lint:fix": "eslint \"db/**/*.ts\" --fix",
    "db:migration:generate": "npx sequelize-cli migration:generate --name",
    "db:migrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate --config db/config/database.js --migrations-path db/migrations",
    "db:migrate:undo:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo --config db/config/database.js --migrations-path db/migrations",
    "db:migrate:undo:all:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all --config db/config/database.js --migrations-path db/migrations",
    "db:migrate:status:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:status --config db/config/database.js --migrations-path db/migrations",
    "db:seed:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all --config db/config/database.js --seeders-path db/seeders",
    "db:seed:undo:all:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:undo:all --config db/config/database.js --seeders-path db/seeders",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create --config db/config/database.js",
    "db:drop:test": "cross-env NODE_ENV=test npx sequelize-cli db:drop --config db/config/database.js",
    "db:reset:test": "npm run db:drop:test && npm run db:create:test && npm run db:migrate:test && npm run db:seed:test",
    "cleanup:ports": "node -e \"if(process.platform==='win32'){require('child_process').exec('powershell -ExecutionPolicy Bypass -File scripts/cleanup-ports.ps1')}else{require('child_process').exec('bash scripts/cleanup-ports.sh')}\"",
    "cleanup:ports:force": "node -e \"if(process.platform==='win32'){require('child_process').exec('powershell -ExecutionPolicy Bypass -File scripts/cleanup-ports.ps1 -Force')}else{require('child_process').exec('bash scripts/cleanup-ports.sh --force')}\"",
    "cleanup:ports:all": "node -e \"if(process.platform==='win32'){require('child_process').exec('powershell -ExecutionPolicy Bypass -File scripts/cleanup-ports.ps1 -AllNodeProcesses')}else{require('child_process').exec('bash scripts/cleanup-ports.sh --all-node')}\"",
    "cleanup:ports:all:force": "node -e \"if(process.platform==='win32'){require('child_process').exec('powershell -ExecutionPolicy Bypass -File scripts/cleanup-ports.ps1 -AllNodeProcesses -Force')}else{require('child_process').exec('bash scripts/cleanup-ports.sh --all-node --force')}\"",
    "start:clean": "npm run cleanup:ports:force && npm run start:dev",
    "ci:check": "npm run lint:ts && npm run build && npm run test:unit && npm run test:integration",
    "ci:check:full": "npm run lint:ts && npm run build && npm run test:unit && npm run test:integration && npm run test:cov"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "module-alias": "^2.2.3",
    "mysql2": "^3.15.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "process": "^0.11.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pino": "^7.0.4",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "jest-html-reporters": "^3.1.7",
    "jiti": "^2.5.1",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "moduleAliases": {
    "@app": "./dist"
  }
}
